{
  "name": "yarsk",
  "version": "1.0.0",
  "description": "My React starter kit. Webpack, Babel, Sass, React 0.13 and more.",
  "main": "app/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start conf/karma.conf.js --single-run",
    "test:watch": "./node_modules/karma/bin/karma start conf/karma.conf.js",
    "test:coverage": "./node_modules/karma/bin/karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "./node_modules/karma/bin/karma start conf/karma.ci.conf.js --single-run",
    "start": "./node_modules/.bin/webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --inline --display-error-details --content-base ./build",
    "build": "./node_modules/.bin/webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist"
  },
  "author": "Brad Daily <brad@bradleyboy.com>",
  "license": "MIT",
  "dependencies": {
    "alphanumeric-id": "^1.0.0",
    "autobind-decorator": "^1.2.0",
    "classnames": "^2.1.1",
    "flummox": "^4.0.0-alpha2",
    "moment": "^2.10.3",
    "object-assign": "^2.0.0",
    "randomcolor": "^0.2.0",
    "react": "~0.13.3",
    "react-dnd": "^1.0.0",
    "react-outlet": "^1.0.1",
    "react-textarea-autosize": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.8",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.13.1",
    "eslint": "^0.21.2",
    "eslint-loader": "^0.11.2",
    "extract-text-webpack-plugin": "^0.8.0",
    "file-loader": "^0.8.3",
    "gh-pages": "^0.3.0",
    "html-webpack-plugin": "git://github.com/ampedandwired/html-webpack-plugin.git",
    "isparta": "^3.0.3",
    "isparta-instrumenter-loader": "^0.2.1",
    "karma": "^0.12.32",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-notify-reporter": "^0.1.1",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.1",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.8",
    "webpack-dev-server": "^1.9.0"
  }
}
